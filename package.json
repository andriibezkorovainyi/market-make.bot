{
  "name": "market-make.bot",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \\\"src/**/*.ts\\\" \\\"test/**/*.ts\\\" --config ./.prettierrc",
    "lint": "eslint  -c .eslintrc.js \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "npm run lint -- --fix",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug 0.0.0.0:9229 --watch",
    "start:prod": "node dist/main",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config test/jest-e2e.json",
    "typeorm": "npx typeorm-ts-node-commonjs -d ./src/models/ormconfig.ts",
    "migration:generate": "npm run typeorm -- migration:generate src/migrations/Migration",
    "migration:create": "npm run typeorm -- migration:create src/migrations/Migration",
    "migration:up": "npm run typeorm -- migration:run",
    "migration:down": "npm run typeorm -- migration:revert",
    "db:seed": "ts-node ./node_modules/typeorm-extension/dist/cli/index.js -d ./src/models/ormconfig seed",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">=18.15.0"
  },
  "dependencies": {
    "@nestjs/axios": "^3.0.0",
    "@nestjs/cache-manager": "^2.1.0",
    "@nestjs/common": "^10.2.7",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.2.7",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/microservices": "^10.2.7",
    "@nestjs/platform-express": "^10.2.7",
    "@nestjs/schedule": "^3.0.4",
    "@sentry/node": "^7.81.1",
    "@sentry/profiling-node": "^1.2.6",
    "accept-language-parser": "^1.5.0",
    "axios": "^1.5.1",
    "bignumber.js": "^9.1.2",
    "cache-manager": "^5.1.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.3",
    "form-data": "^4.0.0",
    "ioredis": "^5.2.4",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.35",
    "nest-winston": "^1.8.0",
    "nestjs-cls": "^3.5.1",
    "nestjs-i18n": "^10.3.6",
    "redlock": "^5.0.0-beta.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.7",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.1.18",
    "@nestjs/schematics": "^10.0.2",
    "@nestjs/testing": "^10.2.7",
    "@types/accept-language-parser": "^1.5.3",
    "@types/cache-manager": "^4.0.4",
    "@types/jest": "29.5.6",
    "@types/lodash": "^4.14.200",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^18.11.9",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "eslint": "^8.51.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-max-params-no-constructor": "^0.0.4",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.0",
    "jest": "29.7.0",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.4",
    "ts-jest": "29.1.1",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.0.0",
    "typescript": "5.1.6"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
